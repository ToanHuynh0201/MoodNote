# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_API_TIMEOUT=30000

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_ENABLE_LOGGING=true

# Feature Flags
EXPO_PUBLIC_ENABLE_SOCIAL_LOGIN=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Third-party Services (Examples - Replace with actual keys)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id_here

# Note: EXPO_PUBLIC_ prefix makes variables accessible in client code
# Never commit actual API keys or secrets to version control
